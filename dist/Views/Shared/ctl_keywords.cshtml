@using mtopt.logic;
<meta name="renderer" content="webkit">
<meta http-equiv="Cache-Control" content="no-siteapp" />
<meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
@if (ViewData["keywords"] != null)
{
    <meta name="keywords" content="@(ViewData["keywords"].ToString())">
}
else
{
    <meta name="keywords" content="@Basis.Parse.Localization("key17")">
}
@if (ViewData["description"] != null)
{
    <meta name="description" content="@(ViewData["description"].ToString())">
}
else
{
    <meta name="description" content="@Basis.Parse.Localization("key17")">
}