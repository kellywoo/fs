@using mtopt.logic;
@using mtopt.model.EntitySales;
@using mtopt.rte.Encrypt;
@{
    ViewBag.Title = Basis.Parse.Localization("complete_withdrawal");
    Layout = "~/Views/Shared/mst_main.cshtml";
    //获取数据
    member_deal Deal = ViewBag.Deal;
}
@section head{
    <style type="text/css">
        .pg-form input,
        .pg-form select{margin-bottom:1em;margin-top:1em;}
    </style>
}
<h3>
    @Basis.Parse.Localization("complete_withdrawal")
</h3>
<div class="row">
    <div class="col-lg-12">
        <p>@Basis.Parse.ToLanguage("您已成功充值" + Deal.money + "元，感谢您的支持")</p>
    </div>
</div>